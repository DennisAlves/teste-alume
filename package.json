{
  "name": "teste-alume",
  "version": "1.0.0",
  "description": "teste alume",
  "main": "index.js",
  "scripts": {
    "start": "ts-node-dev ./src/index.ts",
    "build": "tsc",
    "test": "jest --passWithNoTests --silent --noStackTrace --runInBand",
    "test:verbose": "jest --passWithNoTests --runInBand",
    "test:unit": "npm test -- --watch -c jest-unit-config.js",
    "test:integration": "npm test -- --watch -c jest-integration-config.js",
    "test:staged": "npm test -- --findRelatedTests",
    "test:ci": "npm test -- --coverage"
  },
  "author": "Dennis Alves",
  "license": "ISC",
  "dependencies": {
    "@types/node": "^17.0.21",
    "axios": "^0.26.0",
    "cors": "^2.8.5",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "luxon": "^2.3.1",
    "ts-node": "^10.6.0",
    "ts-node-dev": "^1.1.8",
    "typescript": "^4.6.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/luxon": "^2.3.0",
    "eslint": "^8.10.0",
    "jest": "^27.5.1",
    "jest-extended": "^2.0.0"
  }
}
